<script type="text/javascript">
	$(function () {
	  /* BOOTSNIPP FULLSCREEN FIX */
	  if (window.location == window.parent.location) {
	    $('#back-to-bootsnipp').removeClass('hide');
	  }


	  $('[data-toggle="tooltip"]').tooltip();

	  $('#fullscreen').on('click', function(event) {
	    event.preventDefault();
	    window.parent.location = "http://bootsnipp.com/iframe/4l0k2";
	  });
	  $('a[href="#cant-do-all-the-work-for-you"]').on('click', function(event) {
	    event.preventDefault();
	    $('#cant-do-all-the-work-for-you').modal('show');
	  })

	  $('[data-command="toggle-search"]').on('click', function(event) {
	    event.preventDefault();
	    $(this).toggleClass('hide-search');

	    if ($(this).hasClass('hide-search')) {
	      $('.c-search').closest('.row').slideUp(100);
	    }else{
	      $('.c-search').closest('.row').slideDown(100);
	    }
		})

	  $('#contact-list').searchable({
	    searchField: '#contact-list-search',
	    selector: 'li',
	    childSelector: '.media-body',
	    show: function( elem ) {
	      elem.slideDown(100);
	    },
	    hide: function( elem ) {
	      elem.slideUp( 100 );
	    }
	  })

		$('#group2-list').searchable({
	    searchField: '#group2-list-search',
	    selector: 'li',
	    childSelector: '.media-body',
	    show: function( elem ) {
	      elem.slideDown(100);
	    },
	    hide: function( elem ) {
	      elem.slideUp( 100 );
	    }
	  })
	});

	var chart = c3.generate({
	  bindto: '#chart',
	  title: {
	    text: 'Title'
	  },
	  data: {
	    columns: [
	    ['Group 1', 10000, 12000, 13050, 14000, 16500],
	    ['Group 2', 10000, 13500, 15020, 17050, 18500]
	    ]
	  },
	  axis: {
	    y: {
        label: {
          text: 'Steps',
          position: 'outer-middle'
        },
        max: 20000,
	    },
      x: {
        type: 'category',
        categories: ['4/15 - 4/21', '4/22 - 4/28', '4/29 - 5/5', '5/6 - 5/12', '5/13 - 5/15']
      }
	  },
    legend: {
      show: true
    }
	});
</script>

